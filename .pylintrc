[MASTER]
extension-pkg-whitelist=
ignore=CVS
ignore-patterns=test_.*\.py,.*_test\.py

[MESSAGES CONTROL]
disable=C0111,R0903,C0103

[REPORTS]
output-format=json
reports=no

[REFACTORING]
max-nested-blocks=5
max-returns=6
max-branches=12
max-statements=50

[FORMAT]
max-line-length=100
indent-string='    '

[VARIABLES]
init-import=no
dummy-variables-rgx=_+$|(_[a-zA-Z0-9_]*[a-zA-Z0-9]+?$)|dummy|^ignored_|^unused_

[SIMILARITIES]
min-similarity-lines=4
ignore-comments=yes
ignore-docstrings=yes
ignore-imports=no
